<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Younicorn</title>
    @*Google Fonts*@
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <environment names="Development">
        <link href="~/lib/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />
        <link href="~/lib/Others/icofont/icofont.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
        <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
        <link href="~/lib/font-awesome/css/fontawesome.min.css" rel="stylesheet" />
        <link href="~/lib/animate.css/animate.min.css" rel="stylesheet" />
        <link href="~/lib/OwlCarousel2/assets/owl.carousel.min.css" rel="stylesheet" />
        <link href="~/lib/aos/aos.css" rel="stylesheet" />
        <link href="~/img/favicon.png" rel="icon">
        <link href="~/lib/jquery.datatable/css/jquery.dataTables.min.css" rel="stylesheet" />
        <link href="~/lib/jqueryui/jquery-ui.css" rel="stylesheet" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/jquery-toast-plugin/1.3.2/jquery.toast.min.css" rel="stylesheet" />
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css" />
        <link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css">

    </environment>
    <style>
        input:-internal-autofill-selected {
            appearance: menulist-button;
            background: none !important;
        }
    </style>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/4.6.0/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link href="~/lib/Others/icofont/icofont.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
              asp-fallback-href="~/lib/font-awesome/css/all.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/fontawesome.min.css"
              asp-fallback-href="~/lib/font-awesome/css/fontawesome.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
              asp-fallback-href="~/lib/animate.css/animate.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css"
              asp-fallback-href="~/lib/OwlCarousel2/assets/owl.carousel.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css"
              asp-fallback-href="~/lib/aos/aos.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/datatables/1.10.21/css/jquery.dataTables.min.css"
              asp-fallback-href="~/lib/jquery.datatable/css/jquery.dataTables.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css"
              asp-fallback-href="~/lib/jqueryui/jquery-ui.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        >
    </environment>
    @*Template Main CSS File*@
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/responsive.css" rel="stylesheet">
</head>
<body>
    <header id="header" class="">
        <div class="container">
            <div class="row align-items-center">
                <!-- Uncomment below if you prefer to use an text logo -->
                <!--<h1 class="logo mr-auto"><a href="index.html">Restaurantly</a></h1>-->
                <!-- Uncomment below if you prefer to use an image logo -->
                <div class="col-auto">
                    <a href="/" class="logo mr-auto"><img src="@Url.Content("~/img/logo.png")" alt="" class="img-fluid"></a>
                </div>

                <div class="col">
                    <div class="d-flex justify-content-end align-items-center align-items-md-start">
                        <nav class="nav-menu d-none d-md-block py-3">
                            <ul>
                                <li><a href="@Url.ActionLink("About","Home")">About Us</a></li>
                                <li><a href="@Url.ActionLink("ProviderInfo","Home")">Provider Info</a></li>
                                <li><a href="@Url.ActionLink("Contact","Home")">Contact Us</a></li>
                                <li class="d-md-none"><a href="@Url.ActionLink("Plan","Home")">Compare Plans</a></li>
                                <li class="d-md-none"><a href="#menu">Get Great Internet</a></li>
                            </ul>
                        </nav><!-- .nav-menu -->
                        <!-- .navbar-nav-right -->
                        <ul class="navbar-nav-right d-none d-md-flex align-items-center pt-2 pb-3 px-3 mb-4">
                            <li>
                                <a href="#">
                                    Got a question?<br>
                                    Talk to Younicorn staff now.
                                </a>
                            </li>
                            <li>
                                <a href="#menu"><i class="material-icons">chat</i>&nbsp;Chat</a>
                            </li>
                            <li>
                                <a href="#menu"><i class="material-icons">call</i>&nbsp;Call</a>
                            </li>
                        </ul>
                        <!-- .navbar--nav-right -->
                        <div class="logo-right d-none d-lg-block  pt-2 pb-3">
                            <a href="@Url.ActionLink("login","home")" onclick="window.localStorage.setItem('userid', '');"><img src="@Url.Content("~/img/favicon.png")" alt="" class="img-fluid"></a>
                        </div>
                        <div class="d-md-none">
                            <button type="button" class="mobile-nav-toggle d-lg-none"><i class="icofont-navigation-menu"></i></button>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </header><!-- End Header -->
    <div>
        @RenderBody()
        <hr />
        <footer id="footer" class=" d-none">
            <div class="footer-top">
                <div class="container">
                    <div class="row">

                        <div class="col-lg-3 col-md-6">
                            <div class="footer-info">
                                <h3>Restaurantly</h3>
                                <p>
                                    A108 Adam Street <br>
                                    NY 535022, USA<br><br>
                                    <strong>Phone:</strong> +1 5589 55488 55<br>
                                    <strong>Email:</strong> info@example.com<br>
                                </p>
                                <div class="social-links mt-3">
                                    <a href="#" class="twitter"><i class="bx bxl-twitter"></i></a>
                                    <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
                                    <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
                                    <a href="#" class="google-plus"><i class="bx bxl-skype"></i></a>
                                    <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-2 col-md-6 footer-links">
                            <h4>Useful Links</h4>
                            <ul>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Home</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">About us</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Services</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Terms of service</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Privacy policy</a></li>
                            </ul>
                        </div>

                        <div class="col-lg-3 col-md-6 footer-links">
                            <h4>Our Services</h4>
                            <ul>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Web Design</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Web Development</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Product Management</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Marketing</a></li>
                                <li><i class="bx bx-chevron-right"></i> <a href="#">Graphic Design</a></li>
                            </ul>
                        </div>

                        <div class="col-lg-4 col-md-6 footer-newsletter">
                            <h4>Our Newsletter</h4>
                            <p>Tamen quem nulla quae legam multos aute sint culpa legam noster magna</p>
                            <form action="" method="post">
                                <input type="email" name="email"><input type="submit" value="Subscribe">
                            </form>

                        </div>

                    </div>
                </div>
            </div>

            <div class="container">
                <div class="copyright">
                    &copy; Copyright <strong><span>Restaurantly</span></strong>. All Rights Reserved
                </div>
                <div class="credits">
                    <!-- All the links in the footer should remain intact. -->
                    <!-- You can delete the links only if you purchased the pro version. -->
                    <!-- Licensing information: https://bootstrapmade.com/license/ -->
                    <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/restaurantly-restaurant-template/ -->
                    Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
                </div>
            </div>
        </footer>
        <footer>
            <div class="top-footer">
                <div class="container">
                    <div class="f-logo text-center">
                        <img src="@Url.Content("~/img/logo.png")">
                    </div>
                    <div class="row justify-content-center">
                        <div class="col-md-12 col-lg-4 text-center px-4 mt-5">
                            <i class="icon-map"></i>
                            <h3 class="font-weight-bold text-pumpkin mt-2">Address</h3>
                            <p>
                                G27 Level 2 Annex Building,<br> 27 Gillies Ave. Office Park,<br>Newmarket, Auckland NZ 1023
                            </p>
                        </div>
                        <div class="col-md-6 col-lg-4 text-center px-4 mt-5">
                            <i class="icon-phone"></i>
                            <h3 class="font-weight-bold text-pumpkin mt-2">Phone Number</h3>
                            <p>South Africa : <a href="#">+27 11 447 7337</a></p>
                            <p>USA : <a href="#">+1 512 993 0814</a></p>
                        </div>
                        <div class="col-md-6 col-lg-4 text-center px-4 mt-5">
                            <i class="icon-mail"></i>
                            <h3 class="font-weight-bold text-pumpkin mt-2">Email</h3>
                            <p><a href="#">info@example.com</a></p>
                        </div>

                    </div>
                </div>
            </div>
            <div class="bottom-footer">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="copyright text-center">
                                <p>Copyright © 2019. All Rights Reserved Younicorn Pvt Ltd</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- End Footer -->

        <div id="preloader"></div>
        <a href="#" class="back-to-top"><i class="fa fa-arrow-up"></i></a>
    </div>

    <environment names="Development">
        <script src="@Url.Content("~/lib/jquery/jquery.js")"></script>
        <script src="@Url.Content("~/lib/twitter-bootstrap/js/bootstrap.js")"></script>
        <script src="@Url.Content("~/lib/jqueryui/jquery-ui.js")"></script>
        <script src="@Url.Content("~/lib/jquery.datatable/js/jquery.dataTables.min.js")"></script>
        <script src="@Url.Content("~/js/site.js")" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"
                asp-fallback-src="~/lib/jquery/jquery.min.js"
                asp-fallback-test="window.jQuery"></script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/4.6.0/bootstrap.min.js"
                asp-fallback-src="~/lib/twitter-bootstrap/js/bootstrap.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"></script>
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"
                asp-fallback-src="~/lib/jqueryui/jquery-ui.min.js"
                asp-fallback-test="window.jQueryui"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/datatables/1.10.21/js/jquery.dataTables.min.js"
                asp-fallback-src="~/lib/jquery.datatable/js/jquery.dataTables.min.js"
                asp-fallback-test="window.jQueryDataTables"></script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @*Template Main JS File*@
    <script src="@Url.Content("~/lib/jquery/jquery.js")"></script>
    <script src="@Url.Content("~/lib/jqueryui/jquery-ui.js")"></script>
    <script src="@Url.Content("~/lib/jquery-validation/dist/jquery.validate.js")"></script>

    <script src="@Url.Content("~/lib/jquery-easing/jquery.easing.min.js")"></script>
    <script src="@Url.Content("~/lib/OwlCarousel2/owl.carousel.min.js")"></script>
    <script src="@Url.Content("~/lib/aos/aos.js")"></script>
    <script src="@Url.Content("~/js/main.js")"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-toast-plugin/1.3.2/jquery.toast.min.js"></script>
    <script src="https://unpkg.com/gijgo@1.9.13/js/gijgo.min.js" type="text/javascript"></script>

    @await RenderSectionAsync("scripts", required: false).ConfigureAwait(false)
</body>
</html>
